<%- include ("header"); -%>

  <div class="box" id="heading">
    <h1> <%= listItem %> </h1>
  </div>

  <div class="box">
    <%  for (var i = 0; i < newItems.length; i++) { %>
    <div class="item">
      <input type="checkbox">
      <p> <%= newItems[i] %> </p>
    </div>

    <%  } %>
    <form action="/" method="post">
      <input type="text" name="newTask" placeholder="New Task" autocomplete="off">
      <button type="submit" name="list" value="<%= listItem %>">+</button>
      

    </form>
  </div>

<%- include ("footer"); -%>
